# Suppressions for Dragonfly

##----------------------------------------------------------------------##
# Memcheck
##----------------------------------------------------------------------##

{
   rtld-2
   Memcheck:Cond
   obj:/libexec/ld-elf*.so.*
}
{
	libc cond
	Memcheck:Cond
	obj:/lib/libc.so.*
	fun:readdir
}
{
	libc value
	Memcheck:Value8
	obj:/lib/libc.so.*
}
{
	libc param write
	Memcheck:Param
	write(buf)
	obj:/lib/libc.so.*
}

